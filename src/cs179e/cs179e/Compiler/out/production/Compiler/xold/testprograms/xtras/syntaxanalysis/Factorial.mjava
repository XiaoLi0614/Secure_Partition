
class FactorialMain {
    public static void main(String[] args){
        System.out.println(new Factorial().compute(10));
    }
}

class Factorial {
    public int compute(int num) {
        int result;
        if (num < 1)
            result = 1 ;
        else
            result = num * (this.compute(num - 1));
        return result;
    }
}
